TRUE ~ `Conc`),  # leave unchanged if no match
Run_Date = format(mdy_hm(Run_Time), "%m-%d-%Y")) %>%
filter(!Sample_Name %in% c("Nitrate Standard" , "Nitrite Standard"))
#ID x and y
x1 <- stds_NOx$Absorbance
y1 <- stds_NOx$Conc
stds_NH3 <- stds_NH3 %>%
mutate(`Conc` = case_when(
